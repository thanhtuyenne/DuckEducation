<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>role</param-name>
        <param-value>null</param-value>
    </context-param>
    <servlet>
        <servlet-name>control</servlet-name>
        <servlet-class>Servlet.control</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Student_site</servlet-name>
        <servlet-class>Controller.Student_site</servlet-class>
        <init-param>
            <param-name>student parameter</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Setting_site</servlet-name>
        <servlet-class>Controller.Setting_site</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Log_out</servlet-name>
        <servlet-class>Controller.Log_out</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>Servlet.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>send_code</servlet-name>
        <servlet-class>Servlet.send_code</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Message</servlet-name>
        <servlet-class>Controller.Message</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ForgotPw</servlet-name>
        <servlet-class>Controller.ForgotPw</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Discussion</servlet-name>
        <servlet-class>Controller.Discussion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Study</servlet-name>
        <servlet-class>Controller.Study</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>Controller.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edit</servlet-name>
        <servlet-class>Controller.Edit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfile</servlet-name>
        <servlet-class>Controller.EditProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>new_password</servlet-name>
        <servlet-class>Servlet.new_password</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Servlet.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>control</servlet-name>
        <url-pattern>/control</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Student_site</servlet-name>
        <url-pattern>/student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Setting_site</servlet-name>
        <url-pattern>/setting</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Log_out</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Message</servlet-name>
        <url-pattern>/message</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ForgotPw</servlet-name>
        <url-pattern>/forgotpw</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Discussion</servlet-name>
        <url-pattern>/discussion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Study</servlet-name>
        <url-pattern>/study</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edit</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfile</servlet-name>
        <url-pattern>/editProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>send_code</servlet-name>
        <url-pattern>/send_code</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>new_password</servlet-name>
        <url-pattern>/new_password</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
